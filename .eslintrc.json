{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-console": 0,
        "class-methods-use-this": 0,
        "no-restricted-globals": 0,
        "import/no-extraneous-dependencies": 0,
        "no-param-reassign": 0,
        "prefer-default-export": 0,
    }
}
